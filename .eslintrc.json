{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/extensions": ["error", "ignorePackages", {
            "ts": "never",
            "tsx": "never",
            "js": "always",
            "jsx": "never",
            "mjs": "never"
        }]
    }
}
